{
  "manifest_version": 3,
  "name": "parrot",
  "description": "parrot",
  "version": "1.0.0",
  "action": {
    "default_title": "parrot",
    "default_popup": "index.html",
    "default_icon": {
      "16": "./icons/icon16@1x.png",
      "32": "./icons/icon16@2x.png",
      "48": "./icons/icon16@3x.png",
      "128": "./icons/icon16@8x.png"
    }
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' ; object-src 'self'"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "https://hooks.slack.com/"
  ],
  "icons": {
    "16": "./icons/icon16@1x.png",
    "32": "./icons/icon16@2x.png",
    "48": "./icons/icon16@3x.png",
    "128": "./icons/icon16@8x.png"
  }
}
